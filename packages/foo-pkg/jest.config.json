{
  "transform": {
      ".(ts|tsx)": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.json"
    }
  },
  "testRegex": "(./test/.*-(test|spec))\\.(ts|tsx|js)$",
  "testPathIgnorePatterns": ["/lib/", "/node_modules/"],
  "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
  ]
}
